                   encoding="text"> normal </csymbol>
          <ci> z </ci>
          <cn sbml:units="dimensionless" type="integer"> 10 </cn>
        </apply>
      </math>
    </initialAssignment>
    <initialAssignment symbol="y">
      <notes>
        <body xmlns="http://www.w3.org/1999/xhtml">
          Example: 3.7.2 Defining a truncated normal distribution
        </body>
      </notes>
      <math xmlns="http://www.w3.org/1998/Math/MathML"
            xmlns:sbml="http://www.sbml.org/sbml/level3/version2/core">
        <apply>
          <csymbol definitionURL="http://www.sbml.org/sbml/symbols/distrib/normal"
                   encoding="text"> normal </csymbol>
          <ci> z </ci>
          <cn sbml:units="dimensionless" type="integer"> 10 </cn>
          <apply>
            <minus/>
            <ci> z </ci>
            <cn sbml:units="dimensionless" type="integer"> 2 </cn>
          </apply>
          <apply>
            <plus/>
            <ci> z </ci>
            <cn sbml:units="dimensionless" type="integer"> 2 </cn>
          </apply>
        </apply>
      </math>
    </initialAssignment>
    <initialAssignment symbol="S_amt">
      <math xmlns="http://www.w3.org/1998/Math/MathML">
        <apply>
